#!/usr/bin/env php
<?php

declare(strict_types=1);

use Duyler\Console\ConsoleApplication;

require dirname(__DIR__) . '/vendor/autoload.php';

try {
    $app = new ConsoleApplication();
    $app->run();
    echo "\033[42m Done!";
} catch (Throwable $exception) {
    echo "\033[41m " . $exception->getMessage();
}
