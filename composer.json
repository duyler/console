{
    "name": "duyler/console",
    "type": "library",
    "description": "Minimalistic command-line interface library",
    "license": "MIT",
    "keywords": [
        "command",
        "console",
        "cli"
    ],
    "authors": [
        {
            "name": "Mikhail Ilinsky",
            "email": "mail@milinsky.ru"
        }
    ],
    "support": {
        "docs": "https://duyler.org/en/docs/console/"
    },
    "require": {
        "php": "^8.3",
        "duyler/builder": "dev-main"
    },
    "require-dev": {
        "phpunit/phpunit": "^10.0",
        "friendsofphp/php-cs-fixer": "^3.75",
        "vimeo/psalm": "^6.10"
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "autoload": {
        "psr-4": {
            "Duyler\\Console\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Duyler\\Console\\Test\\": ["tests/"]
        }
    },
    "bin": [
        "bin/do"
    ]
}
